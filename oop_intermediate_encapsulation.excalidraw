      "text": "#include <iostream>\n\nclass AkunBank {\n    private:\n    std::string nomorAkun;\n    double tabungan;\n    \npublic:\n    AkunBank(std::string noAkun, double jumlah) {\n        nomorAkun = noAkun;\n        tabungan = jumlah;\n    }\n    \n    double lihat_saldo() {\n        return tabungan;\n    }\n    \n    std::string lihat_nomor_akun() {\n        return nomorAkun;\n    }\n    \n    void tabung(double jumlah) {\n        if (jumlah > 0) {\n            tabungan += jumlah;\n            std::cout << \"berhasil tabung sebanyak Rp.\" << jumlah << std::endl;\n            std::cout << \"jumlah uang adalah Rp.\" << tabungan << std::endl;\n        } else {\n            std::cout << \"jumlah harus lebih dari 0\" << std::endl;\n        }\n    }\n    \n    void tarik(double jumlah) {\n        if (jumlah > 0 && jumlah <= tabungan) {\n            tabungan -= jumlah;\n            std::cout << \"tarik berhasil sebanyak Rp.\" << jumlah << std::endl;\n            std::cout << \"jumlah uang sekarang adalah Rp.\" << tabungan << std::endl;\n        } else {\n            std::cout << \"tidak boleh mengisi kurang dari nol\" << std::endl;\n        }\n    }\n};\n\nint main() {\n    AkunBank akun(\"12799\", 100000);\n    std::cout << \"nomor akun: \" << akun.lihat_nomor_akun() << std::endl;\n    std::cout << \"sisa saldo: Rp.\"  << akun.lihat_saldo() << std::endl;\n    \n    akun.tabung(300000);\n}",
